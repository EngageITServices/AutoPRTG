{
  "sensors": [
    {
      "internal_sensorId": 17,
      "name": "Misc Data/Log",
      "type": "pccustom",
      "tags": "performancecounter performancecountercustom",
      "priority": "3",
      "InheritInterval": 0,
      "interval": 300,
      "primary_channel": 3,
      "channels": [
        {
          "channelId": 2,
          "name": "\\PhysicalDisk(5)\\% Disk Time",
          "measure_unit": "%",
          "limit_type": "none",
          "properties": {}
        },
        {
          "channelId": 3,
          "name": "\\PhysicalDisk(5)\\Avg. Disk Queue Length",
          "measure_unit": "n",
          "limit_type": "upper",
          "properties": {
            "upper_error_limit": 15,
            "upper_warning_limit": 10,
            "lower_error_limit": null,
            "lower_warning_limit": null,
            "error_limit_message": "Average Queue length is too high",
            "warning_limit_message": "Average Queue length is high"
          }
        },
        {
          "channelId": 4,
          "name": "\\PhysicalDisk(5)\\Avg. Disk sec/Read",
          "measure_unit": "IO/sec",
          "limit_type": "upper",
          "properties": {
            "upper_error_limit": 30,
            "upper_warning_limit": 20,
            "lower_error_limit": null,
            "lower_warning_limit": null,
            "error_limit_message": "Avg. Disk Reads is too high",
            "warning_limit_message": "Avg. Disk Reads is high"
          }
        },
        {
          "channelId": 5,
          "name": "\\PhysicalDisk(5)\\Avg. Disk sec/Write",
          "measure_unit": "IO/sec",
          "limit_type": "upper",
          "properties": {
            "upper_error_limit": 30,
            "upper_warning_limit": 20,
            "lower_error_limit": null,
            "lower_warning_limit": null,
            "error_limit_message": "Avg. Disk Writes is too high",
            "warning_limit_message": "Avg. Disk Writes is high"
          }
        },
        {
          "channelId": 6,
          "name": "\\PhysicalDisk(5)\\Disk Reads/sec",
          "measure_unit": "IO/sec",
          "limit_type": "none",
          "properties": {}
        },
        {
          "channelId": 7,
          "name": "\\PhysicalDisk(5)\\Disk Writes/sec",
          "measure_unit": "IO/sec",
          "limit_type": "none",
          "properties": {}
        },
        {
          "channelId": 8,
          "name": "\\PhysicalDisk(5)\\Current Disk Queue Length",
          "measure_unit": "n",
          "limit_type": "none",
          "properties": {}
        }
      ]
    }
  ]
}